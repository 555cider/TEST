extends layout.pug

block append output
    section.locations
        h2 대여소
        table(style='width:100%', border='1').locations-result
            tr 
                th ID 
                th Located Bike Count 
            for location of locations
                tr
                    td= location.id
                    td= location.located_bikes_count 
    br    
    section.trucks
        h2 트럭
        table(style='width:100%', border='1').trucks-result
            tr 
                th ID 
                th Loaded Bike Count 
                th Location ID
            for truck of trucks
                tr
                    td= truck.id
                    td= truck.loaded_bikes_count 
                    td= truck.location_id